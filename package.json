{
  "name": "mutate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm compress && vue-tsc --noEmit && vite build && pnpm run pack",
    "preview": "vite preview",
    "compress": "ts-node-esm script/compress.ts",
    "pack": "ts-node-esm script/pack.ts",
    "buildh5": "pnpm compress && pnpm build && ts-node-esm script/packh5.ts",
    "updateh5": "pnpm buildh5 && ts-node-esm update.ts",
    "build-gh": "pnpm compress && vue-tsc --noEmit && vite build --base=/Mutate-HTML/ && pnpm run pack"
  },
  "dependencies": {
    "ant-design-vue": "^3.2.13",
    "axios": "^1.1.2",
    "less": "^4.1.3",
    "lodash": "^4.17.21",
    "mutate-game": "^1.0.1",
    "typed.js": "^2.0.12",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@types/fontmin": "^0.9.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "18.8.0",
    "@vitejs/plugin-legacy": "^2.2.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "compressing": "^1.6.2",
    "fontmin": "^0.9.9",
    "fs-extra": "^10.1.0",
    "terser": "^5.15.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "unplugin-vue-components": "^0.22.8",
    "vite": "^3.1.0",
    "vue-tsc": "1.0.8"
  }
}